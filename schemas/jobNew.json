{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "title",
        "company_handle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "salary": {
            "type": ["integer", "null"],
            "default": null
        },
        "equity": {
            "type": ["number", "null"],
            "default": null,
            "maximum": 1.0
        },
        "company_handle": {
            "type": "string"
        }
    },
    "additionalProperties": false
}